{
  "name": "kinopio-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.vue"
  },
  "dependencies": {
    "@aguezz/qs-parse": "^2.0.0",
    "@stripe/stripe-js": "^1.7.0",
    "dayjs": "^1.9.1",
    "fuzzy": "^0.1.3",
    "lodash-es": "^4.17.15",
    "nanoid": "^2.0.3",
    "overlap-area": "^1.0.0",
    "randomcolor": "^0.5.4",
    "shader": "^1.0.0",
    "smooth-scroll-into-view-if-needed": "^1.1.23",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.3",
    "@vue/devtools": "6.0.0-beta.12",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.18.0",
    "lint-staged": "^11.1.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "@vue/compiler-sfc": "^3.2.6",
    "vite": "^2.5.7",
    "@vitejs/plugin-vue": "^1.6.2",
    "postcss": "^8.3.6",
    "autoprefixer": "^10.3.4",
    "pug": "^3.0.2",
    "yorkie": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "no-throw-literal": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix"
    ]
  }
}
